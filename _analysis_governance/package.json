{
	"name": "governance-system-template",
	"private": true,
	"packageManager": "bun@1.3.7",
	"engines": {
		"bun": "1.3.7",
		"node": "24.x"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"typescript": "5.9.3",
		"vitest": "4.0.18",
		"dprint": "0.51.1"
	},
	"scripts": {
		"lockfile:check": "bun ./scripts/js-lockfile-law.ts",
		"biome:fmt": "biome format --write .",
		"biome:fmt:check": "biome format .",
		"biome:lint": "biome lint --write .",
		"biome:lint:check": "biome lint .",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest run --passWithNoTests",
		"typecheck:node": "node ./node_modules/typescript/bin/tsc -p tsconfig.json --noEmit",
		"test:node": "node ./node_modules/vitest/vitest.mjs run --passWithNoTests"
	}
}
